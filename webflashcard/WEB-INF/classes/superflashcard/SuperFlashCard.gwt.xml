<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='superflashcard'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.StandardResources'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/>-->
  <inherits name ="com.google.gwt.xml.XML"/>
<!--    <set-property name="user.agent" value="safari" /> -->
  <!-- Other module inherits                                      -->
  <inherits name="com.google.web.bindery.event.Event"/>
  <inherits name='com.google.gwt.search.Search' />
  <inherits name="com.google.gwt.resources.Resources"/>
  <inherits name="pl.rmalinowski.gwt2swf.GWT2SWF"/>
	<inherits name="com.google.gwt.i18n.I18N"/>
	<inherits name="com.googlecode.objectify.Objectify"/>
	<inherits name="com.reveregroup.gwt.imagepreloader.ImagePreloader" />
	<inherits name='com.allen_sauer.gwt.voices.gwt-voices'/>
	<inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" />

<!-- <script src="//www.google.com/uds/api?file=uds.js&amp;v=1.0&amp;gwt=1"/> -->

  <!-- Load dinamically predefined styles in the library when the application starts -->
  <!--  <stylesheet src="Upload.css"/> -->
  
  
    <!-- FOR ANDROID -->
<!--   <set-property name="user.agent" value="safari" />
  <collapse-property name="mobile.user.agent" values="*"/>
  <set-property name="mgwt.os" value="android" /> -->
  <!-- END FOR ANDROID -->

<!--<inherits name="com.google.gwt.activity.Activity" />-->
  <add-linker name="xsiframe"/>
  <set-property name="user.agent" value="safari" />
  <set-configuration-property name="devModeRedirectEnabled" value="true"/>
  <set-property name="compiler.useSourceMaps" value="true" />
  <set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
  
  <!-- Specify the app entry point class.                         -->
  <entry-point class='superflashcard.client.SuperFlashCard'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
</module>